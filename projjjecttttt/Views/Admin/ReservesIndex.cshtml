@model IEnumerable<projjjecttttt.Models.Reserve>

@{
    ViewData["Title"] = "Index";
}

<pre>





</pre>

<h1>OUR RESERVES</h1>
<a asp-controller="Home" asp-action="Index" class="btn btn-outline-info">Back To Home</a>

<table class="table  table-striped table-bordered table-hover">
    <thead>
        <tr class="bg-primary text-white">
            <th>
                @Html.DisplayNameFor(model => model.Checkin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Checkout)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GuestNum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdImg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Unit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Checkin)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Checkout)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GuestNum)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdImg)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Unit.ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User.Id)
                </td>
                <td>
                    <a asp-action="ReservesEdit" asp-route-id="@item.Id" class="btn btn-warning">Edit</a> |
                    <a asp-action="ReservesDetails" asp-route-id="@item.Id" class="btn btn-info">Details</a> |
                    <a asp-action="ReservesDelete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>|
                </td>
            </tr>
        }
    </tbody>
</table>


